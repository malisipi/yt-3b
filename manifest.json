{
	"name": "YT|3B",
	"description": "YT ADBlocker",
	"version": "0.2.2",
	"manifest_version": 3,
	"icons": {
		"16": "/assets/16.png",
		"32": "/assets/32.png",
		"48": "/assets/48.png",
		"128": "/assets/128.png",
		"144": "/assets/144.png",
		"192": "/assets/192.png",
		"512": "/assets/512.png"
	},
	"author": "malisipi",
	"content_scripts": [
		{
			"matches": [
				"https://*.youtube.com/*"
			],
			"js": ["3b.js"]
		}
	]
}
